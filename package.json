{
  "name": "vue-component-tree",
  "version": "1.3.0",
  "description": "Tree view for your demo components",
  "main": "index.js",
  "scripts": {
    "build": "npm run dist && npm run demo",
    "dist": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack.config.dist.js",
    "demo": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack.config.demo.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --open --config webpack.config.dev.js",
    "test": "karma start --single-run",
    "test-dev": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asvae/vue-component-tree.git"
  },
  "keywords": [
    "vue",
    "tree",
    "demo"
  ],
  "author": "Yauheni Prakopchyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asvae/vue-component-tree/issues"
  },
  "homepage": "https://github.com/asvae/vue-component-tree#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "brace": "^0.10.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "~3.0.0",
    "file-loader": "^0.11.0",
    "flow-bin": "~0.50.0",
    "flow-remove-types": "^1.2.1",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^2.29.0",
    "jasmine-core": "^2.6.1",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.1.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-requirejs": "^1.1.0",
    "karma-webpack": "^2.0.3",
    "node-libs-browser": "^2.0.0",
    "node-sass": "^4.5.2",
    "requirejs": "^2.3.3",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "vue": "^2.4.0",
    "vue-loader": "^12.2.1",
    "vue-router": "^2.5.3",
    "vue-template-compiler": "^2.3.3",
    "webpack": "~3.2.0",
    "webpack-dev-server": "^2.5.0"
  }
